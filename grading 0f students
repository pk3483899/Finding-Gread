#include<stdio.h>
#include<conio.h>
int main()
{
    int m1,m2,m3,m4,m5,per;
    printf("Enter the marks");
    scanf("%d%d%d%d%d",&m1,&m2,&m3,&m4,&m5);

    per=(m1+m2+m3+m4+m5)*100/500;
    if(per>=85)
    printf("Gread A");
    else if(per>=70)
    printf("Gread B");
    else if(per>=55)
    printf("Gread C");
    else if(per>40)
    printf("Gread D");
    else if(per<40)
    printf("Gread F");
    return 0;
}
